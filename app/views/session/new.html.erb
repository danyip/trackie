<div class="page-wrapper">


  <h1>Login</h1>

  <% if flash[:error] %>
    <br>
      <h3><%= flash[:error] %></h3>
    <br> <br>
  <% end %>

  <%= form_with url: login_path, local: true do |f| %>

    <%= f.label :email %>
    <br>
    <%= f.text_field :email %>
    <br><br>
  
    <%= f.label :password %>
    <br>
    <%= f.password_field :password %>
    <br><br>
    <%= f.submit 'Login' %>
  <% end %>


</div>